Actor ArmorBonusDS2 : AetheriusCustomInventory
{
	Game Doom
	Radius 20
	Height 16
	Inventory.Pickupsound "getarmorbonus" 
	Inventory.Pickupmessage "Picked up an \cdarmor bonus\c-."
	Inventory.Icon "ARM1A1"
	+FLOATBOB
	+DONTGIB
	+COUNTITEM
	+INVENTORY.ALWAYSPICKUP
	States
	{
		Spawn:
			BON2 A 0 NoDelay
			{
				ACS_NamedExecuteAlways("ArmorBonusEyeGlow",0,0,0,0);
				if (random(1,2048) <= 1)
				{
					A_SpawnItemEx("MonsterMunchies",0,0,0,0,0,0,0,0,0);
				}
			}
			BON2 A 0 A_Jump(1,"AddSpawn")    
			goto Float
		AddSpawn:
			BON2 A 0 A_Jump(1,"AddSpawn2")    
			goto Float
		AddSpawn2:
			TNT1 A 0 A_SpawnItemEx("RandomArmorSpawner",0,0,0,0,0,0,0,0,0)  
			goto Float

		Float:
			BON2 AABBCCDDCCBB 3 BRIGHT 
			{
				ACS_NamedExecuteAlways("ArmorBonusEyeGlow",0,0,0,0);
			}
			loop

		Pickup:
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 0, "Success")
			TNT1 A 0 A_JumpIf(CallACS("CheckCharacterClass") == 1, "Success")
			Fail
		Success:
			TNT1 A 0 { CallACS("EmulateGiveArmorBonus",0,0,0); }
			Stop
	}
}

actor ArmorBonus2 : ArmorBonus replaces ArmorBonus
{
	//$Sprite "BON2A0"
	Radius 8
	Height 8
	-COUNTITEM
	-INVENTORY.ALWAYSPICKUP
	States
	{
		SpawnManaSmall:
			TNT1 A 0 A_SpawnItemEx("DoomSpellMana",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0,tid)
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		SpawnManaMedium:
			TNT1 A 0 A_SpawnItemEx("DoomSpellMana2",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0,tid)
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		SpawnManaLarge:
			TNT1 A 0 A_SpawnItemEx("DoomSpellMana3",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0,tid)
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		SpawnHealthBonus:
			TNT1 A 0 A_SpawnItemEx("HealthBonus",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0,tid)
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		SpawnMana:
			TNT1 A 0 A_Jump(1, "SpawnManaLarge")
			TNT1 A 0 A_Jump(16, "SpawnManaMedium")
			TNT1 A 0 A_Jump(256, "SpawnManaSmall")
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		FloraDetected: // bonus will be turned into a HealthBonus/DoomspellMana item instead
			TNT1 A 0 A_Jump(8, "SpawnMana")
			TNT1 A 0 A_Jump(256, "SpawnHealthBonus")
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		FloraDetectedClassicDoom2: // bonus will be turned into something else [plus a very rare chance for an armor item to drop]
			TNT1 A 0 A_Jump(8, "SpawnMana")
			TNT1 A 0 A_Jump(255, "SpawnHealthBonus")
			TNT1 A 0 A_SpawnItemEx("FloraRandomArmorDoom2a",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0,tid)
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		FloraNotDetected: // bonus will be turned into a ArmorBonusDS2 instead
			TNT1 A 0 A_SpawnItemEx("ArmorBonusDS2",0,0,0,0,0,0,0,SXF_TRANSFERAMBUSHFLAG | SXF_TRANSFERSPECIAL,0,tid)
			TNT1 A 0 A_FadeOut(1.0)
			Loop

		Spawn:
			TNT1 A 0 NoDelay
			{
				if (CallACS("ClassicDoom2ArmorCheck") == 0) { return state("FloraNotDetected"); } 
				else if (CallACS("ClassicDoom2ArmorCheck") == 1) { return state("FloraNotDetected"); } 
				else if (CallACS("ClassicDoom2ArmorCheck") == 2) { return state("FloraDetected"); } 
				else if (CallACS("ClassicDoom2ArmorCheck") == 3) { return state("FloraNotDetected"); } 
				else if (CallACS("ClassicDoom2ArmorCheck") == 4) { return state("FloraNotDetected"); } 
				else if (CallACS("ClassicDoom2ArmorCheck") == 5) { return state("FloraDetectedClassicDoom2"); } 
				return state(""); 
			}
			loop
	}
}

