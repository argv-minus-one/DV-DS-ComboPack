			goto Float
		PaxaSpawn2:
			TNT1 A 0 A_SpawnItemEx("PaxaPunch",0,0,0,0,0,0,0,0,0)  
			goto Float
		MercBandanaSpawn:
			BON1 A 0 A_Jump(104,"MercBandanaSpawn2")    
			goto Float
		MercBandanaSpawn2:
			TNT1 A 0 A_SpawnItemEx("MercuryBandana",0,0,0,0,0,0,0,0,0)  
