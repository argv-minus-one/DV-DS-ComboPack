// Pyro Demon Walking Fire
actor WalkFire
{
     DamageType "Fire"
     +NOCLIP
     +MISSILE
     +DROPOFF
     +RANDOMIZE
     scale 1.5
     Speed 1
     RENDERSTYLE ADD
     ALPHA 0.67
     States
     {
     Spawn:
        TNT1 A 8
        FRTF A 3 Bright
        FRTF A 0 
        FRTF B 3 Bright
        FRTF B 0 
        FRTF C 3 Bright
        FRTF C 0 
        FRTF C 0 
        FRTF C 0 A_Jump(191,2)
        FRTF C 0
		{
		 if (GetCVar("dvds_gfxeffectlevel") == 0)
		 {
	 	  A_CustomMissile("WalkFire", 0, 24, 0);
          A_CustomMissile("WalkFire", 0, 0, 0);
          A_CustomMissile("WalkFire", 0, -24, 0);
		 }
		 else
		 if (GetCVar("dvds_gfxeffectlevel") == 1)
		 {
          A_CustomMissile("WalkFire", 0, 0, 0);
		 }
		}
        FRTF DEFGHIJKLMNO 3 Bright
        Stop
     }
}

